
#include <iostream>
#include <string>
using namespace std;
class Book {
public:
	string title;
	string autor;
	void get() {
		getline(cin,title);
		getline(cin, autor);

		/*title = "Great C++";
		autor = "c++";*/
	}
	void draw() {
		
		cout << "책 이름 :" << title << endl;
		cout << "책 저자 : " << autor << endl;
	}
};
int main()
{
	Book book;
	cout << "책 이름과 저자 입력: ";
	book.get();

	book.draw();




	/*
	책 이름과 저자 입력: Great C++
Bob
책 이름 :Great C++
책 저자 : Bob
*/


}
